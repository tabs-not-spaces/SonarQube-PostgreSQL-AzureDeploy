{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "East US"
    },
    "postgresAdminUsername": {
      "value": "sonaradmin"
    },
    "postgresAdminPassword": {
      "value": "CHANGE_ME_SecurePassword123!"
    },
    "postgresServerName": {
      "value": "sonarqube-postgresql-prod"
    },
    "databaseName": {
      "value": "sonarqube"
    },
    "containerGroupName": {
      "value": "sonarqube-containers-prod"
    },
    "dockerHubUsername": {
      "value": "YOUR_DOCKERHUB_USERNAME"
    },
    "dockerHubPassword": {
      "value": "YOUR_DOCKERHUB_PASSWORD_OR_TOKEN"
    },
    "sonarQubeVersion": {
      "value": "community"
    },
    "nginxVersion": {
      "value": "alpine"
    },
    "cpuCores": {
      "value": 4
    },
    "memoryInGb": {
      "value": 8
    },
    "tags": {
      "value": {
        "Application": "SonarQube",
        "Environment": "Production",
        "Owner": "DevOps Team",
        "CostCenter": "Engineering",
        "Backup": "Required"
      }
    }
  }
}